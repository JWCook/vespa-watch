{
  "name": "vespa-watch",
  "description": "Website for the monitoring of Vespa velutina",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "create:css": "node-sass static_src/scss --output-style nested --source-map true --output vespawatch/static/vespawatch/main.css",
    "watch:css": "onchange 'static_src/scss/*.scss' -- npm run create:css"
  },
  "dependencies": {
    "bootstrap": "4.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1"
  }
}
